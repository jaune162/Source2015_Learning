<?xml version="1.0" encoding="UTF-8"?>
<mapping>
        <class name="com.tiamaes.learning.spring.bean.XMLBean">
  
           <map-to xml="xmlbean"/>
  
           <field name="age" type="integer">
              <bind-xml name="age" node="element"/>
           </field>
           <field name="name" type="string">
              <bind-xml name="name" node="element"/>
           </field>
        </class>
</mapping>